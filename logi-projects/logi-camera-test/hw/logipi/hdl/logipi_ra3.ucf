######################
# Timing Constraints #
######################

NET OSC_FPGA TNM_NET = clk50_grp;
#NET SDRAM_CLK TNM_NET = clk100_grp;
NET SYS_SPI_SCK TNM_NET = clk32_grp;

##### Clock Period Constraints #####
TIMESPEC TS_PER_CLK50 = PERIOD "clk50_grp" 20.0 ns ;
